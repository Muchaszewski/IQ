<Window x:Class="Creator.MessageBoxInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MessageBoxInput" Height="104" Width="306">
    <Grid>
        <TextBox x:Name="TextBoxInput" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272" TabIndex="0"/>
        <Button x:Name="ButtonOk" Content="Ok" HorizontalAlignment="Left" Margin="207,38,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="ButtonOk_Click"/>
        <Button x:Name="ButtonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="127,38,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" Click="ButtonCancel_Click"/>
    </Grid>
</Window>
